{
  "name": "travel-port1",
  "version": "1.0.0",
  "description": "Travel aid App by Victor Godwin",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "NODE_ENV=production babel-node ./src/index.js",
    "start:heroku": "NODE_ENV=production npm run db-migrate && babel-node ./src/index.js",
    "test": "NODE_ENV=testing npm run db-undo-migration && NODE_ENV=testing npm run db-migrate && NODE_ENV=testing nyc --reporter=html --reporter=text mocha --require @babel/register src/tests/*.js  --require @babel/polyfill --require @babel/register --timeout 100000000 --exit",
    "pretest-travis": "NODE_ENV=testing npm run db-undo-migration && NODE_ENV=testing  npm run db-migrate",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "db-migrate": "cross-env NODE_ENV=production npx sequelize db:migrate",
    "db-undo-migration": "npx sequelize db:migrate:undo:all",
    "db-seed-test": "cross-env NODE_ENV=testing npx sequelize-cli db:seed:undo:all && NODE_ENV=testing sequelize db:seed:all",
    "db-seed-dev": "npx sequelize-cli db:seed:undo:all && npx sequelize db:seed:all"
  },
  "repository": "https://github.com/cstudio7/travel-port1.git",
  "author": "Victor Godwin <conquerorsword36@yahoo.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/register": "^7.10.1",
    "@babel/runtime": "^7.10.1",
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "@sendgrid/mail": "^7.1.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.4.0",
    "moment": "^2.26.0",
    "node-mocks-http": "^1.8.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.11",
    "sequelize-cli": "^5.5.1",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@passport-next/chai-passport-strategy": "^1.1.0",
    "coverage": "^0.4.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.1"
  }
}
